{
  "name": "@leimingyun/translate-react",
  "description": "一个基于 i18n-jsautotranslate 的 React 翻译组件",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/translate-react.cjs.js",
  "module": "./dist/translate-react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/translate-react.es.js",
      "require": "./dist/translate-react.cjs.js"
    },
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "i18n",
    "translate",
    "translation",
    "internationalization",
    "multilingual",
    "i18n-jsautotranslate"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --config vite.lib.config.ts && tsc --project tsconfig.lib.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build:lib"
  },
  "dependencies": {
    "i18n-jsautotranslate": "^3.17.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
